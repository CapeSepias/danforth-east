{% extends "base.jinja" %}

{% set page_title = 'Member Management Login' %}

{% block title %}{{ page_title }}{% endblock %}

{# Get rid of the header on this page #}
{% block header %}{% endblock %}

{% block content %}

<div class="jumbotron">
  <h1>Please Log In</h1>
  <br>
  <div id="my-signin2"></div>
  <br>
  <div id="signin_failed" hidden>
    Sign in failed: <span id="signin_failed_reason"></span>
  </div>
</div>

<input id="csrf_token" type="hidden" class="hidden" value="{{ csrf_token() }}" />

{% endblock %}<!-- content -->

{% block pagescript_file %}
<script src="/js/login.js"></script>
{% endblock %}
